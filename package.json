    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2"
  },
  "dependencies": {
    "@doist/todoist-api-typescript": "^3.0.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "dompurify": "^3.0.6",
    "marked": "^11.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
